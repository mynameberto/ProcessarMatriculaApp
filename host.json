{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "cors": {
    "allowedOrigins": ["*"],
    "allowedMethods": ["POST", "OPTIONS"],
    "allowedHeaders": ["Content-Type", "Accept", "Origin", "X-Requested-With"],
    "maxAge": 3600
  }
}